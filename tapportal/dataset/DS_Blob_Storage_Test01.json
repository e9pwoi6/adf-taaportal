{
	"name": "DS_Blob_Storage_Test01",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_Blob_Storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"File Name": {
				"type": "String",
				"defaultValue": "'varicent ' + getdate()"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat('varicent-test/',formatDateTime(utcNow(), 'yyyy-MM-dd mm:ss'), '.yourfileextension')",
					"type": "Expression"
				},
				"container": "varicent-test"
			},
			"columnDelimiter": "|",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "'"
		},
		"schema": []
	}
}