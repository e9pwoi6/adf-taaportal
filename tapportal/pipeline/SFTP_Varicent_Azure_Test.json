{
	"name": "SFTP_Varicent_Azure_Test",
	"properties": {
		"activities": [
			{
				"name": "Clear_Storage",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "SFTP Varicent File",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_Blob_Storage_Test",
						"type": "DatasetReference"
					},
					"enableLogging": false,
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true,
						"wildcardFileName": "Pramata_*.txt",
						"enablePartitionDiscovery": false
					}
				}
			},
			{
				"name": "SFTP Varicent File",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "Pramata_*.txt",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "SftpWriteSettings",
							"operationTimeout": "01:00:00",
							"useTempFileRename": true
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Blob_Storage_Test",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SFTP_Varicent_Outbound",
						"type": "DatasetReference",
						"parameters": {
							"Directory": "/Data"
						}
					}
				]
			}
		],
		"parameters": {
			"Directory": {
				"type": "String",
				"defaultValue": "/Data"
			}
		},
		"annotations": []
	}
}