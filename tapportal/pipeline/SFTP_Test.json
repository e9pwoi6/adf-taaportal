{
	"name": "SFTP_Test",
	"properties": {
		"activities": [
			{
				"name": "Copy_Varicent_Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT  doc.DocumentID \t,REPLACE(doc.CompanyGroupName, '|', '') AS CompanyGroupName ,REPLACE(doc.DocumentType, '|', '') AS DocumentType \t,doc.EffectiveDate\n,trm.TerminationDate \t,NULL AS MCFID \t,doc.ParentDocumentID \t,NULLIF(hdr.DealProfileID, '') AS DealProfileID  ,'Pramata_11102018_062140' AS BatchID \nFROM \tFoundational.vwDocumentIndex doc  JOIN SellSide.vwDocument_TerminationDate trm  \nON trm.DocumentID = doc.DocumentID \t AND trm.TerminationDate IS NOT NULL \tLEFT JOIN PramataFileTransfer.dbo.FileTransferHeader hdr \t\nON doc.DocumentID = hdr.PramataNumber WHERE \tdoc.SubClass IN ('CPH','MHS','SA')  AND doc.DocumentType NOT IN ('MCF','Associated Materials') ",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_CCTAAP_005",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Blob_Storage_Test01",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Clear_Storage",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "SFTP Varicent File",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_Blob_Storage_Test",
						"type": "DatasetReference",
						"parameters": {
							"Directory": {
								"value": "@pipeline().parameters.Directory",
								"type": "Expression"
							}
						}
					},
					"enableLogging": false,
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			},
			{
				"name": "SFTP Varicent File",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Copy_Varicent_Data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "Paramat_*.txt",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Blob_Storage_Test01",
						"type": "DatasetReference",
						"parameters": {
							"File_Name": "@concat('Pramata_', formatDateTime(utcNow(), 'yyyyMMdd_hhmmss'), '.txt')"
						}
					}
				]
			}
		],
		"parameters": {
			"Directory": {
				"type": "String",
				"defaultValue": "/mckesson2-sftp-us-east-1/mckesson2_to_pramata/mckesson2_to_pramata/Mckesson_to_Pramata_UAT"
			}
		},
		"annotations": []
	}
}